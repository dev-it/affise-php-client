{
    "name": "devit/affise-php-client",
    "description": "A client for Affise API",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.0",
        "guzzlehttp/guzzle": "^6.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0@dev"
    },
    "autoload": {
        "classmap": [
            "src/Status.php"
        ],
        "psr-4": {
            "Bytelovers\\Affise\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bytelovers\\Affise\\Test\\": "tests/"
        }
    },
    "minimum-stability": "dev"
}
